    "rich_text_h8WfPt": {
      "type": "rich-text",
      "blocks": {
        "heading_yLycpe": {
          "type": "heading",
          "settings": {
            "heading": "Biggest Selection of Sunscreens in Dubai, UAE  ",
            "heading_size": 30,
            "heading_size_mb": 30,
            "heading_color": "#000"
          }
