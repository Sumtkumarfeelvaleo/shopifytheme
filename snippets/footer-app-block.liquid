<script>
    var site_nav_link_burger = true;
    var site_nav_link_search = true;
    var site_nav_link_auth = true;
    var site_nav_link_cart = true;
    var site_atc = true;
    var site_atc_buttons = true;
    document.addEventListener("DOMContentLoaded", function() {
        var icon_menu = document.querySelector('[data-mobile-menu]');
        var icon_search = document.querySelector('.header-mobile .header__icon--search');
        var icon_auth = document.querySelector('.header-mobile [data-open-auth-sidebar]');
        var icon_cart = document.querySelector('.header-mobile [data-cart-sidebar]');
        var icon_atc = document.querySelector('.productView-product .product-form__submit');
        if(icon_menu){
          icon_menu.addEventListener("click", function() {
              if(site_nav_link_burger && !document.documentElement.classList.contains('scld')) {
                  event.preventDefault();
                  site_nav_link_burger = false;
              }
          });
        }
        if(icon_search){
          icon_search.addEventListener("click", function() {
              if(site_nav_link_search && !document.documentElement.classList.contains('scld')) {
                  event.preventDefault();
                  site_nav_link_search = false;
              }
          });
        }
        if(icon_auth){
          icon_auth.addEventListener("click", function() {
              if(site_nav_link_auth && !document.documentElement.classList.contains('scld')) {
                  event.preventDefault();
                  site_nav_link_auth = false;
              }
          });
        }
        if(icon_cart){
          icon_cart.addEventListener("click", function() {
              if(site_nav_link_cart && !document.documentElement.classList.contains('scld')) {
                  event.preventDefault();
                  site_nav_link_cart = false;
              }
          });
        }
        if(icon_atc){
          icon_atc.addEventListener("click", function() {
              if(site_atc && !document.documentElement.classList.contains('scld')) {
                  event.preventDefault();
                  site_atc = false;
              }
          });
        }
      if(document.querySelectorAll('.productListing .product-form__submit').length > 0){
        document.querySelectorAll('.productListing .product-form__submit').forEach(button => {
            button.addEventListener('click', function(event) {
              if(site_atc_buttons && !document.documentElement.classList.contains('scld')) {
                event.preventDefault();
                window.clicked_button = this;
                site_atc_buttons = false;
              }
            });
        });  
      }
    }) 
</script>  

